fn classify(n: int) -> int {
    if n < 0 { return -1; }
    else if n == 0 { return 0; }
    else if n < 10 { return 1; }
    else { return 2; }
}

fn main() -> int {
    let a: int = classify(-5);
    let b: int = classify(0);
    let c: int = classify(7);
    let d: int = classify(20);

    let result: int = a + b + c + d + 1;

    log.logInfo("Result:");
    printInt(result);
    return 0;
}
