fn classify(n: Int) -> Int {
    let result: Int = 0;

    switch (n) {
        case 0: {
            result = 0;
            break;
        }
        case 1: {
            result = 10;
            break;
        }
        case 2: {
            result = 20;
            break;
        }
        default: {
            result = -1;
            break;
        }
    }

    return result;
}

fn main() -> Int {
    let x: Int = 5;
    let r: Int = classify(x);
    return r;
}
