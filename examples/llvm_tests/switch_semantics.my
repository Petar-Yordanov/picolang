import log;

fn classify(n: int) -> int {
    let result: int = 0;

    switch (n) {
        case 0: {
            result = 0;
            break;
        }
        case 1: {
            result = 10;
            break;
        }
        case 2: {
            result = 20;
            break;
        }
        default: {
            result = -1;
            break;
        }
    }

    return result;
}

fn main() -> int {
    let x: int = 5;
    let r: int = classify(x);

    log.logInfo("Result:");
    printInt(r);

    return 0;
}
