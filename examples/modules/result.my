enum Result[T, E] {
    Ok(T),
    Err(E),
}

fn ok[T, E](value: T) -> Result[T, E] {
    return Result.Ok(value);
}

fn err[T, E](error: E) -> Result[T, E] {
    return Result.Err(error);
}

fn defaultIfErr[T, E](res: Result[T, E], fallback: T) -> T {
    switch res {
        case Result.Ok(value) {
            return value;
        }
        case Result.Err(_) {
            return fallback;
        }
    }
}
