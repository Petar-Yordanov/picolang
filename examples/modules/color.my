enum Color {
    Red,
    Green,
    Blue,
    Rgb(byte, byte, byte),
}

fn isBright(c: Color) -> bool {
    switch c {
        case Color.Red {
            return true;
        }
        case Color.Green {
            return true;
        }
        case Color.Blue {
            return false;
        }
        case Color.Rgb(r, g, b) {
            return r + g + b > 128 * 3;
        }
    }
}
