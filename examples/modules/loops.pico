fn loopsDemo() -> void {
    let i: int = 0;
    while i < 3 {
        printInt(i);
        i = i + 1;
    }

    let sum: int = 0;
    for (let j: int = 0; j < 5; j = j + 1) {
        sum = sum + j;
    }
    printInt(sum);

    let k: int = 0;
    for (; k < 3; k = k + 1) {
        printInt(k);
    }
}
