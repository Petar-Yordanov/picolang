class Point {
    x int;
    y int;

    fn translate(dx: int, dy: int) -> Point {
        let result: Point;
        result.x = this.x + dx;
        result.y = this.y + dy;
        return result;
    }

    fn manhattanLength() -> int {
        return abs(this.x) + abs(this.y);
    }
}

fn newPoint(x: int, y: int) -> Point {
    let p: Point;
    p.x = x;
    p.y = y;
    return p;
}
